<body ng-app="LoginMod" ng-controller="LoginCtrl" class="container">

<div class="modal-dialog">
  <div class="loginmodal-container">
    <h1>Login to Your Account</h1><br>
    <form ng-submit="runLogin()">
      <input type="email" required="required" ng-model="email"  name="email" placeholder="Email" autofocus="autofocus">
      <input type="password" required="required" ng-model="password" placeholder="Password">
      <input type="submit" name="login" class="login loginmodal-submit green" value="Login">
    </form>

    <div class="">
      <input type="submit" ng-click="register()" name="register" class="login loginmodal-submit" value="Register">

    </div>
  </div>
</div>

